---
import Layout from '~/layouts/PageLayout.astro';
import HeroText from '~/components/widgets/HeroText.astro';
import Features3 from '~/components/widgets/Features3.astro';
import Program from '~/components/program.astro'

const metadata = {
  title: 'โปรแกรมการแข่งขัน',
};
---

<Layout metadata={metadata}>
  <!-- HeroText Widget ******************* -->

  <HeroText
    title="โปรแกรมการแข่งขัน"
    subtitle="การแข่งขันกีฬาระหว่างคณะ นาคาเกมส์"
  />


  <!-- Features3 Widget ************** -->

  <Features3
    title="-โปรแกรมการแข่งขันยังไม่สมบูรณ์-"
    subtitle="อยู่ระหว่างการนำข้อมูลโปรแกรมการแข่งขันลงระบบ"
    columns={2}
    classes={{ container: 'max-w-5xl' }}
  />

  <Program />


</Layout>
